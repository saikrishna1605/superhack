{
  "version": 2,
  "builds": [
    {
      "src": "services/ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "services/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "services/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "services/ui/$1"
    }
  ]
}
